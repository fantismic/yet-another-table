{
    "name": "fantismic/yet-another-table",
    "version": "1.4.0",
    "description": "This is yet another laravel livewire table and come as is. You can filter, you can sort, you can bulk, toggle columns, the basics. The data input is a collection/array, we cant handle models.",
    "license": "MIT",
    "keywords": [
        "livewire table",
        "table",
        "blade components",
        "laravel components",
        "livewire components"
    ],
    "require": {
        "php": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Fantismic\\YetAnotherTable\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Fantismic"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Fantismic\\YetAnotherTable\\YATProvider"
            ]
        }
    }
}
